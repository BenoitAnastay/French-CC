        "maps/arid/interior/gautham-room.json/entities/31/settings/event/16/text": {
                "orig": "That was bound to happen!",
                "text": "Il fallait que ça arrive !"
        },
        "maps/arid/interior/gautham-room.json/entities/31/settings/event/17/text": {
                "orig": "The odds were still highly \nstacked against them.",
                "text": "Mais les chances n'étaient\nvraiment pas de leur côté."
