        "extension/post-game/data/maps/evo-village/interior/lea.json/entities/69/settings/event/73/message": {
                "orig": "An Evotar...\\. of \\c[3]C'tron\\c[0].",
                "text": "Un Évotar...\\. de \\c[3]C'tron\\c[0]."
        },
