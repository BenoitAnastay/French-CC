        "maps/shock-dng/g/room1.json/entities/58/settings/event/5/message": {
                "orig": "Good luck, everyone!",
                "text": "Bonne chance, tout le monde !"
        },
        "maps/shock-dng/g/room1.json/entities/58/settings/event/6/message": {
                "orig": "You don't have to wish me luck. Just run slow and I'll be happy.",
                "text": "Tu n'as pas besoin de me souhaiter bonne chance. Vas-y lentement et je serais contente."
        },
        "maps/shock-dng/g/room1.json/entities/58/settings/event/7/message": {
                "orig": "I won't give you that luxury.",
                "text": "Je ne vais pas te laisser ce luxe."
        },
