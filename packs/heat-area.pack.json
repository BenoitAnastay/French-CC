        "maps/heat/dng-expo-space.json/entities/4/settings/event/8/text": {
                "orig": "The everlasting sun \nshines strongest on these lands.",
                "text": "Le soleil éternel\nbrille au plus fort sur ces terres."
        },
        "maps/heat/dng-expo-space.json/entities/4/settings/event/9/text": {
                "orig": "Its flaming glory bearing\nlife and death alike.",
                "text": "Sa splendeur enflammée apportant\ntant la vie que la mort."
        },
        "maps/heat/dng-expo-space.json/entities/4/settings/event/10/text": {
                "orig": "On this unforgiving path \nyou stayed persistent.",
                "text": "Sur ce chemin ardu\nvous avez prouvé votre persévérance."
        },
        "maps/heat/dng-expo-space.json/entities/4/settings/event/11/text": {
                "orig": "With frozen sweeps and strokes\nyou crossed the scorching grounds.",
                "text": "Avec des frappes et des trainées de gel\nVous avez traversé les sols ardents."
        },
        "maps/heat/dng-expo-space.json/entities/4/settings/event/12/text": {
                "orig": "You, the traveler, \nthe envoy of change.",
                "text": "Vous, le voyageur.\nL'émissaire du changement."
        },
        "maps/heat/dng-expo-space.json/entities/4/settings/event/13/text": {
                "orig": "You have proven yourself worthy.",
                "text": "Vous avez prouvé votre dignité."
        },
        "maps/heat/dng-expo-space.json/entities/4/settings/event/14/text": {
                "orig": "Take this Shade \nto open the path to the east.",
                "text": "Prenez ce Symbole\npour ouvrir le chemin à l'est."
        },
        "maps/heat/dng-expo-space.json/entities/4/settings/event/21/object": {
                "orig": "\\v[item.230.name]",
                "text": "Symbole de Flamme Rouge"
        },
        "maps/heat/dng-expo-space.json/entities/4/settings/event/23/text": {
                "orig": "Your resolve is evident\nand deserving a gift of strength.",
                "text": "Votre détermination est évidente\net mérite un présent de force."
        },
        "maps/heat/dng-expo-space.json/entities/4/settings/event/24/text": {
                "orig": "Let me add to your inner flame\nto make its warmth long lasting.",
                "text": "Laissez-moi renforcer votre flamme intérieure\npour que sa chaleur dure plus longtemps."
        },
